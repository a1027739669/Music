    function tuijianSheet() {
        toList(1,1);
    }
    function newSheet() {
        toList(1,2);
    }
    function toList(page,rankmethod) {
        $.ajax({
            url: '/guest/sheettable',
            type: 'GET',
            data: {
                'rankMethod':rankmethod,
                'pageId': page,
            },
            success: function (data) {
                $("#playlistAll").empty();
                $("#playlistAll").append(data);
            }
        });
    }
